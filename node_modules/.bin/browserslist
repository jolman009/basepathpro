#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules/.pnpm/browserslist@4.27.0/node_modules/browserslist/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules/.pnpm/browserslist@4.27.0/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules/.pnpm/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/node_modules:/proc/cygdrive/c/Users/Jolma/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/Jolma/vibe-code/my-basepath-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules/.pnpm/browserslist@4.27.0/node_modules/browserslist/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules/.pnpm/browserslist@4.27.0/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules/.pnpm/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/node_modules:/proc/cygdrive/c/Users/Jolma/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/Jolma/vibe-code/my-basepath-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/browserslist@4.27.0/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/browserslist@4.27.0/node_modules/browserslist/cli.js" "$@"
fi
