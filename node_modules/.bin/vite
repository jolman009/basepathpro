#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules/.pnpm/vite@5.4.21_@types+node@24.10.0/node_modules/vite/bin/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules/.pnpm/vite@5.4.21_@types+node@24.10.0/node_modules/vite/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules/.pnpm/vite@5.4.21_@types+node@24.10.0/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules/.pnpm/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/node_modules:/proc/cygdrive/c/Users/Jolma/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/Jolma/vibe-code/my-basepath-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules/.pnpm/vite@5.4.21_@types+node@24.10.0/node_modules/vite/bin/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules/.pnpm/vite@5.4.21_@types+node@24.10.0/node_modules/vite/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules/.pnpm/vite@5.4.21_@types+node@24.10.0/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules/.pnpm/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/my-basePath-app/node_modules:/proc/cygdrive/c/Users/Jolma/Vibe-Code/node_modules:/proc/cygdrive/c/Users/Jolma/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/Jolma/vibe-code/my-basepath-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vite@5.4.21_@types+node@24.10.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../.pnpm/vite@5.4.21_@types+node@24.10.0/node_modules/vite/bin/vite.js" "$@"
fi
